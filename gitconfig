[color]
        diff = auto
        status = auto
        branch = auto
[alias]
        c = commit
        s = status
        d = diff
        dc = diff --cached
        p = push
        a = add
	b = branch
	ff = merge
	m = merge --no-ff
	co = checkout
	cp = cherry-pick
[core]
	excludesfile = ~/.gitignore
	pager = less -r
[push]
	default = simple
